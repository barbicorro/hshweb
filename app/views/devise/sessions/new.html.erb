<div class="container-fluid bg-dark pb-4">
  <div class="text-center"> 
        <h2 class="text-white mb-3">Iniciar sesion</h2>
        <div class="alert text-white mb-3 alert-expand-lg bg-dark"><%= notice%> </div>
  

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs text-white ">
    <%= f.input :email,
                required: false,
                autofocus: true,
                input_html: { autocomplete: "Direccion de correo electronico" } %>
    <%= f.input :password,
                required: false,
                input_html: { autocomplete: "ContraseÃ±a" } %>
    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Entrar" %>
  </div>
<% end %>

<%= render "users/shared/links" %>
</div>

</div>